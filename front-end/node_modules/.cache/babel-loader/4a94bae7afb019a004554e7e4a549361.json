nsole.log(\"update \"+record);\n  },\n  newProfile(){\n    this.$router.push({ path: this.$route.params.id2+'/form'});\n  }\n  }\n}\n</script>\n\n<!-- ============================================================================ -->\n\n<style>\nb-button{\n  margin: 5px;\n  width: 100px;\n}\n#newBtn{\n  position:absolute;\n  transition: .5s ease;\n  top: 11%;\n  left: 93%;\n}\n</style>\n"],"sourceRoot":"src/components"}]}