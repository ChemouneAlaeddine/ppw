bel>\n          <input type=\"text\" class=\"form-control\" placeholder=\"Prénom\" v-model=\"student.surname\">\n        </div>\n        <div class=\"form-group\">\n          <label class=\"pull-left\"> Email </label>\n          <input type=\"text\" class=\"form-control\" placeholder=\"Email\" v-model=\"student.email\">\n        </div>\n        <div class=\"form-group\">\n          <label class=\"pull-left\"> Nom complet </label>\n          <input type=\"text\" class=\"form-control\" placeholder=\"Nom Complet\" v-model=\"student.fullname\">\n        </div>\n        <div class=\"form-group\">\n          <label class=\"pull-left\"> Moyenne </label>\n          <input type=\"text\" class=\"form-control\" placeholder=\"Moyenne\" v-model=\"student.float\">\n        </div>\n      </div>\n  \n      <button type=\"submit\" class=\"btn btn-primary\">Valider</button>\n      <button type=\"reset\" class=\"btn btn-primary\" @click=\"onReset\">Annuler</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  data () {\n    return {\n      student: {\n        campus: 0,\n        uf: 0,\n        name: \"\",\n        surname: \"\",\n        email: \"\",\n        fullname: \"\",\n        float: 0\n      },\n      show: true\n    }\n  },\n  created() {\n    var i = this.$route.params.id1 - 1;\n    this.student.uf = this.$route.params.id2;\n\n    axios\n      .get(\"http://localhost:3000/data/campus\")\n      .then(response => {\n        this.student.campus = response.data[i].name.split(\" \")[response.data[i].name.split(\" \").length - 1].toLowerCase();\n      });\n  },\n  methods: {\n    addToAPI() {\n      let newStudent = {\n        campus: this.student.campus,\n        uf: this.student.uf,\n        name: this.student.name,\n        surname: this.student.surname,\n        email: this.student.email,\n        fullname: this.student.fullname,\n        float: this.student.float\n      }\n      let indx = this.$route.params.id3;\n      axios\n      .get(\"http://localhost:3000/data/\"+this.student.campus+\"_\"+this.student.uf)\n      .then(response => {\n          console.log(response.data[indx]);\n          axios\n            .put('http://localhost:3000/put/'+this.student.campus+'/'+this.student.uf+'/'+indx, newStudent);\n            window.location.reload();\n      });\n    },\n    onReset (evt) {\n      evt.preventDefault();\n      this.student.email = '';\n      this.student.name = '';\n      this.student.surname = '';\n      this.student.float = 0;\n      this.show = false;\n      this.$nextTick(() => { this.show = true });\n    }\n  }\n}\n</script>\n\n<style scoped>\n.container{\n  width: 50%;\n  margin: auto;\n}\nbutton{\n  margin-left: 60px;\n  text-align: center;\n}\n</style>"]}]}